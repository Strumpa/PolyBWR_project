#!/bin/sh
#
if [ $# = 0 ]
   then
   echo '$$$ ----- OECD_NEA_PHASE_IIIB_post_treatment.save : script begin' 1>&2
   exit 1
fi
echo '$$$ ----- OECD_NEA_PHASE_IIIB_post_treatment.save : script begin'
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi

cp -r OECD_NEA_IIIB_results $1/"$MACH"/

echo '$$$ ----- OECD_NEA_PHASE_IIIB_post_treatment.save : script finish'

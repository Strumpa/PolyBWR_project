#!/bin/sh
#
if [ $# = 0 ]
   then
   echo '$$$ ----- AT10_24UOX_DRAGON_DOP_VOID.save : script begin' 1>&2
   exit 1
fi
echo '$$$ ----- AT10_24UOX_DRAGON_DOP_VOID.save : script begin'
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi

cp *.csv DRAGON5_validation_for_mphys
cp -r DRAGON5_validation_for_mphys $1/"$MACH"/
echo '$$$ ----- AT10_24UOX_DRAGON_DOP_VOID.save : script finish'

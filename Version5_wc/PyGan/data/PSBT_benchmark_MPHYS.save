#!/bin/sh
#
if [ $# = 0 ]
   then
   echo '$$$ ----- PSBT_benchmark_MPHYS.save : script begin' 1>&2
   exit 1
fi
echo '$$$ ----- PSBT_benchmark_MPHYS.save : script begin'
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi

mkdir DISTR_res
mv Flux*.res DISTR_res/
mv Pdistr.res DISTR_res/
cp -r DISTR_res ./PSBT_benchmark_MPHYS_PyGan_PSBT_bench
cp -r PSBT_benchmark_MPHYS_PyGan_PSBT_bench $1/"$MACH"/

echo '$$$ ----- PSBT_benchmark_MPHYS.save : script finish'

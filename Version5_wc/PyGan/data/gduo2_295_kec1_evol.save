#!/bin/sh
#
if [ $# = 0 ]
   then
   echo '$$$ ----- gduo2_295_kec1_evol.save : script begin' 1>&2
   exit 1
fi
echo '$$$ ----- gduo2_295_kec1_evol.save : script begin'
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi

cp _COMPO* ./gduo2_295_kec1_evol_results_PyGan/
cp *CPO* ./gduo2_295_kec1_evol_results_PyGan/
cp -r gduo2_295_kec1_evol_results_PyGan $1/"$MACH"/
echo '$$$ ----- gduo2_295_kec1_evol.save : script finish'

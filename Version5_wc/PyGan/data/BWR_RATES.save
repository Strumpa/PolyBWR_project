#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "-----.save----- BWR_RATES save script begun" 1>&2
   exit 1
fi
echo "-----.save----- BWR_RATES save script begun"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
cp *.ps ./DRAGON_ASS*
cp -r DRAGON_ASS* $1/"$MACH"/
echo "-----.save----- BWR_RATES save script terminated"

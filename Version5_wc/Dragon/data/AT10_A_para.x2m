LIBRARY2 := MIX_AUTOP LIBRARY TRACK_SS TF_EXC_SS :: <<ssh_method>> <<ssh_sol>> ;

FLUX_CALC LIBRARY2 TRACK TF_EXC :: <<flx_sol>> ;


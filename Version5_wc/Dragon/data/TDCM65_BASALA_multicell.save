#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: TDCM65 (BASALA) mulicell.save directory" 1>&2
   exit 1
fi
echo "access TDCM65 (BASALA) mulicell.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv Fic_SAL_TDCM65_IC.tdt $1/"$MACH"
mv Fic_SAL_TDCM65_IC.ps $1/"$MACH"
mv Fic_SAL_TDCM65_CP.tdt $1/"$MACH"
mv Fic_SAL_TDCM65_CP.ps $1/"$MACH"
mv Fic_SAL_TDCM65_MOC.tdt $1/"$MACH"
mv Fic_SAL_TDCM65_MOC.ps $1/"$MACH"
echo "TDCM65 (BASALA) mulicell.save completed"


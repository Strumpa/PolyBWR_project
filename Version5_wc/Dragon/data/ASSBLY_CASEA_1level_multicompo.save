#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: ASSBLY_CASEA_1level_multicompo.save directory" 1>&2
   exit 1
fi
echo access ASSBLY_CASEA_1level_multicompo.save
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv MyCompo.txt $1/"$MACH"/assbly_CASEA_multicompo_draglib.txt
mv *.ps $1/"$MACH"
echo "ASSBLY_CASEA_1level_multicompo.save completed"

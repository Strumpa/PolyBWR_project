#!/bin/sh
if [ $# = 0 ]
   then
   echo "usage: AT10_GLOW_tests.access directory" 1>&2
   exit 1
fi
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
MACH=`uname -o`
elif [ $Sysx = "Darwin" ]; then
MACH=`uname -sm`
elif [ $Sysx = "SunOS" ]; then
MACH=`uname -sm`
fi
if [ "$MACH" = "Linux" -o "$MACH" = "OSF1" -o "$MACH" = "Cygwin" -o "$MACH" = "SunOS i86pc" -o \
"$MACH" = "Darwin i386" -o "$MACH" = "Darwin x86_64" -o "$MACH" = "Darwin_arm64" ]
then 
  echo 'use little endian libraries'
  pos=$1/../../../libraries/l_endian
else
  echo 'use big endian libraries'
  pos=$1/../../../libraries/b_endian
fi
if [ -f "$pos"/draglibJeff3p1p1SHEM295.gz ]
then
echo 'gunzipping draglibJeff3p1p1SHEM295'
chmod 755 "$pos"
gunzip "$pos"/draglibJeff3p1p1SHEM295.gz
fi
if [ -f "$pos"/draglibJeff3p1p1SHEM295 ]
then
ln -s "$pos"/draglibJeff3p1p1SHEM295 J311_295
echo "AT10_GLOW_tests access script terminated"
fi
pos2="$1/../../../glow_data/tdt_data"
chmod 755 "$pos2"
ls -la
ln -s "$pos2"/AT10_simple_cell_TSPC.dat AT10_simple_cell_TSPC.dat
ln -s "$pos2"/AT10_simple_cell_TISO.dat AT10_simple_cell_TISO.dat

ln -s "$pos2"/AT10_2x2_cells_TSPC.dat AT10_2x2_cells_TSPC.dat
ln -s "$pos2"/AT10_2x2_cells_TISO.dat AT10_2x2_cells_TISO.dat
ln -s "$pos2"/AT10_2x2_TISO_MACRO.dat AT10_2x2_TISO_MACRO.dat

ln -s "$pos2"/AT10_3x3_cells_TSPC.dat AT10_3x3_cells_TSPC.dat
ln -s "$pos2"/AT10_3x3_cells_TISO.dat AT10_3x3_cells_TISO.dat
ln -s "$pos2"/AT10_3x3_TISO_MACRO.dat AT10_3x3_TISO_MACRO.dat

ln -s "$pos2"/AT10_4x4_cells_TSPC.dat AT10_4x4_cells_TSPC.dat
ln -s "$pos2"/AT10_4x4_cells_TISO.dat AT10_4x4_cells_TISO.dat
ln -s "$pos2"/AT10_4x4_TISO_MACRO.dat AT10_4x4_TISO_MACRO.dat

ln -s "$pos2"/AT10_lattice_TSPC.dat AT10_lattice_TSPC.dat
ln -s "$pos2"/AT10_lattice_TISO.dat AT10_lattice_TISO.dat
ln -s "$pos2"/AT10_LAT_TISO_MACRO.dat AT10_LAT_TISO_MACRO.dat

ln -s "$pos2"/AT10_lattice_MODEBOX_TSPC.dat AT10_lattice_MODEBOX_TSPC.dat
ln -s "$pos2"/AT10_lattice_MODEBOX_TISO.dat AT10_lattice_MODEBOX_TISO.dat
ln -s "$pos2"/AT10_LAT_MB_TISO_MACRO.dat AT10_LAT_MB_TISO_MACRO.dat

ln -s "$pos2"/AT10_assembly_TSPC.dat AT10_assembly_TSPC.dat
ln -s "$pos2"/AT10_assembly_TISO.dat AT10_assembly_TISO.dat
ln -s "$pos2"/AT10_assembly_TISO_MACRO.dat AT10_assembly_TISO_MACRO.dat
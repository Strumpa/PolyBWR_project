#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: HOM_UOX_Gd157_AUTOlib_test.save directory" 1>&2
   exit 1
fi
echo "access HOM_UOX_Gd157_AUTOlib_test.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv COMPO_295_FINE_Gd157 $1/"$MACH"
echo "HOM_UOX_Gd157_AUTOlib_test.save completed"


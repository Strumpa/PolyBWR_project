#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: OECD_NEA_IIIB.save directory" 1>&2
   exit 1
fi
echo "access OECD_NEA_IIIB.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv Fig_PhaseIIIB*.ps $1/"$MACH"
mv Fig_PhaseIIIB_SSH*.ps $1/"$MACH"
mv _CPO_OECD_NEA_PHASE_IIIB* $1/"$MACH"
echo "OECD_NEA_IIIB.save completed"


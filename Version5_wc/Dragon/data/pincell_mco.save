#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: pincell_mco.save directory" 1>&2
   exit 1
fi
echo access pincell_mco.save
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv ACOMPO $1/"$MACH"/_ACompo_pincell
echo "pincell_mco.save completed"

#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: GE14_GLOW_tests.save directory" 1>&2
   exit 1
fi
echo "access GE14_GLOW_tests.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l

mv *.ps $1/"$MACH"
mv *GEO*.dat $1/"$MACH"
echo "GE14_GLOW_tests.save completed"


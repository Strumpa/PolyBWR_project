#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: GEOM_TEST.save directory" 1>&2
   exit 1
fi
echo "access GEOM_TEST.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l

mv 'GEOM_FIG_TEST.ps' $1/"$MACH"
echo "GEOM_TEST.save completed"


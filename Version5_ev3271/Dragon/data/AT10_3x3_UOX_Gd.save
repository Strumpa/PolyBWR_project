#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: AT10_3x3_UOX.save directory" 1>&2
   exit 1
fi
echo "access AT10_3x3_UOX_Gd.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l

mv FIG_main_3.ps $1/"$MACH"
mv FIG_selfshielding_3.ps $1/"$MACH"
mv tracks_SALT_FLX_3x3_UOX.m $1/"$MACH"
mv tracks_SALT_SSH_3x3_UOX.m $1/"$MACH"
echo "AT10_3x3_UOX_Gd.save completed"


       pu240 = '2407001' 8.7E-04 CORR 1 IRSET PTSL 1
       '16'       7.074E-02
       '1'        5.0E-02
     ;
LIBRARY2 := USS: LIBRARY DISCRF :: EDIT 3 PASS 3 ;

Pij := DELETE: Pij ;
     
Pij := ASM: LIBRARY_N TRACK_T INTLIN_T :: 
      EDIT 0
      PIJ        
      PNOR HELI     
      ;
      
FLUX1 := FLUX ;
FLUX1 := FLU: FLUX1 Pij LIBRARY_N TRACK_T ::
      TYPE K B0TR BUCK IDEM ; 

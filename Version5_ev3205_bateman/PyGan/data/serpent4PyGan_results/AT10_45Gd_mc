% --- Problem title:
set title "AT10_45Gd Pin cell benchmark"

% --- Cross section library file path:

set acelib "../xs/jeff311/sss_jeff311u.data"

% --- Decay data library

set declib "../xs/jeff311/sss_jeff311.dec"

% --- Neutron-induced fission yield library

set nfylib "../xs/jeff311/sss_jeff311.nfy"



% --- 1. Pin definition

pin 1
GADO_A  0.19834
GADO_B  0.28049
GADO_C  0.34353
GADO_D  0.39668
GADO_E  0.43227
GADO_F  0.4435
gap   0.4520
clad   0.5140
H2O   

surf 10 sqc 0.0 0.0 0.6475
cell 1 0  fill 1   -10
cell 2 0  outside   10

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%



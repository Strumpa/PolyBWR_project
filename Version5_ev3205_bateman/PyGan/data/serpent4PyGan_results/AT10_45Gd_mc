mat Zr96  1.0 tmp 559.0 40096.03c 1.0

ene 2 1 1.1E-11 6.25E-7 1.9640E+1

det _FLUX_2G         de 2  

set ures no

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
%             FUEL POWER 2 GROUPS (detectors)
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

det _PIN_A   de 2   dm GADO_A
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241
det _PIN_B   de 2   dm GADO_B
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241
det _PIN_C   de 2   dm GADO_C
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241
det _PIN_D   de 2   dm GADO_D
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241
det _PIN_E   de 2   dm GADO_E
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241
det _PIN_F   de 2   dm GADO_F
    dr 102 U235
    dr 102 U238
    dr 102 Pu239
    dr 102 Pu241
    dr -6 U235
    dr -6 U238
    dr -6 Pu239
    dr -6 Pu241

% end of detectors definition

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% --- 5. Serpent parameters

% --- 5.1 Thermal scattering data for light water:

therm  lwtr lwj3.11t

% --- 5.2 Reflective boundary condition:

set bc 2

% --- 5.2.2 Symmetry condition :
set sym 4

% --- 5.3 Neutron population and criticality cycles:
set bumode 2  % CRAM method
set pcc 1     % Predictor-corrector calculation on
set powdens 38.6E-3
set gcu 0
set nfg 2 0.625E-6
set pop 16000 500 20
plot 3 500 500
mesh 3 500 500



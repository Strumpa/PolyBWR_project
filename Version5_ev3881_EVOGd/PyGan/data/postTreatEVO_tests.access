#!/bin/sh
if [ $# = 0 ]
   then
   echo "-----.access----- postTreatEVO_tests access script begun" 1>&2
   exit 1
fi
echo "-----.access----- postTreatEVO_tests access script begun"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "Darwin" ]; then
   MACH=`uname -sm | sed 's/[ ]/_/'`
elif [ $Sysx = "SunOS" ]; then
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ln -s $DRAGON_RESULTS DRAGON_COMPOS_path
ln -s $DRAGON_RESULTS_EVOGd EVOGd_modif_path
ln -s $PYGAN_RESULTS_EVOGd EVOGd_modif_path_PyGan
ls -l
echo -"----.access----- postTreatEVO_tests access script terminated"

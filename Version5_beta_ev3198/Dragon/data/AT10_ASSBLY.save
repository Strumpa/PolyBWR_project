#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: AT-10_pin.save directory" 1>&2
   exit 1
fi
echo "access AT-10_pin.save"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
mv tracks_SALT_ASSBLY.m $1/"$MACH"
mv tracks_SALT_SS_ASSBLY.m $1/"$MACH"
mv FIG_main.ps $1/"$MACH"
mv FIG_selfshielding.ps $1/"$MACH"
echo "AT10_assmbly.save completed"


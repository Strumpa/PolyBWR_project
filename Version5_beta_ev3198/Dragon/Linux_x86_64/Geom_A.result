INTEGER GAP CLAD COOL ;                                                                                                    0078
EVALUATE GAP CLAD COOL := 5 6 7 ;                                                                                          0079
! Assembly level common mixes water, channel and box                                                                       0080
! Careful : some intra assembly water is treated at the 2nd geometry level                                                 0081
! MODE is associated with H20 OUTSIDE assembly box and INSIDE channel box, the rest is COOL                                0082
INTEGER BOX MODE ;                                                                                                         0083
EVALUATE BOX MODE := 60 70 ;                                                                                               0084
!--> check compositions for box and channel, the same --> Yes !                                                            0085
! Treating Cladding and Boxes with different mixes but outer box and channel are the same !                                0086
! Evaluating dependencies                                                                                                  0087
! Multiples of cell pitch : used to define 2nd level geometry mesh                                                         0088
REAL 2_Pitch_C 3_Pitch_C 4_Pitch_C ;                                                                                       0089
EVALUATE 2_Pitch_C := Pitch_C 2.0 * ;                                                                                      0090
EVALUATE 3_Pitch_C := Pitch_C 3.0 * ;                                                                                      0091
EVALUATE 4_Pitch_C := Pitch_C 4.0 * ;                                                                                      0092
! X1 = Water gap + outer box thickness +extra water : in box water not a multiple of Pitch_C                               0093
REAL XtraMode ;                                                                                                            0094
EVALUATE XtraMode := Box_in 10.0 Pitch_C * - 2.0 / ;                                                                       0095
!Meshing points for macro geometry :                                                                                       0096
REAL X1 X2 X3 X4  ;                                                                                                        0097
! X2 = X1 + 4*Pitch_C                                                                                                      0098
! X3 = X2 + 3*Pitch_C                                                                                                      0099
! X4 = X3 + 3*Pitch_C                                                                                                      0100
EVALUATE X1 := W_gap Box_thi + XtraMode + ;                                                                                0101
EVALUATE X2 := 4.0 Pitch_C * X1 + ;                                                                                        0102
EVALUATE X3 := 3.0 Pitch_C * X2 + ;                                                                                        0103
EVALUATE X4 := 3.0 Pitch_C * X3 + ;                                                                                        0104
REAL Y1 Y2 Y3 Y4 ;                                                                                                         0105
EVALUATE Y1 := X1 ;                                                                                                        0106
EVALUATE Y2 := 3.0 Pitch_C * Y1 + ;                                                                                        0107
EVALUATE Y3 := 3.0 Pitch_C * Y2 + ;                                                                                        0108
EVALUATE Y4 := 4.0 Pitch_C * Y3 + ;                                                                                        0109
REAL L1 L2 L3 ;                                                                                                            0110
EVALUATE L1 := X2 X1 - ;                                                                                                   0111
EVALUATE L2 := X3 X2 - ;                                                                                                   0112
EVALUATE L3 := X4 X3 - ;                                                                                                   0113
REAL H1 H2 H3 ;                                                                                                            0114

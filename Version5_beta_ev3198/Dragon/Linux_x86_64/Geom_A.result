MODULE  GEO: END: ;                                                                                                        0036
!Assembly scale data                                                                                                       0037
REAL Pitch_A W_gap := 15.24 0.75 ; ! Assembly pitch and water gap thickness                                                0038
REAL Box_out Box_in Box_thi := 13.74 13.4 0.17 ; ! Outer box outer, inner sides and thickness                              0039
REAL Chan_out Chan_in Chan_thi := 3.5 3.34 0.16 ; ! Channel box outer, inner sides and thickness                           0040
! UOX Pincell scale data                                                                                                   0041
REAL Pitch_C := 1.29500 ;                                                                                                  0042
REAL Rgap Rclad pitch := 0.4520 0.5140 1.29500 ;                                                                           0043
REAL Rcomb1 Rcomb2 Rcomb3 Rcomb4 ;                                                                                         0044
EVALUATE Rcomb4 := 0.4435 ;                                                                                                0045
EVALUATE Rcomb1 := 0.313602 ;                                                                                              0046
EVALUATE Rcomb2 := 0.396678 ;                                                                                              0047
EVALUATE Rcomb3 := 0.43227 ;                                                                                               0048

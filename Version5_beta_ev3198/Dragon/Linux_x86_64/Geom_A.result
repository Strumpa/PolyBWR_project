EVALUATE XCHNL2 := XCHNL1 Chan_thi + ;                                                                                     0129
EVALUATE XCHNL3 := XCHNL2 Chan_in + ;                                                                                      0130
EVALUATE XCHNL4 := XCHNL3 Chan_thi + ;                                                                                     0131
EVALUATE XCHNL5 := XCHNL4 XCHNL1 + ;                                                                                       0132

#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "-----.save----- postTreatEVO_tests save script begun" 1>&2
   exit 1
fi
echo "-----.save----- postTreatEVO_tests save script begun"
MACH=`uname -s`
Sysx="`echo $MACH | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s`
else
   MACH=`uname -sm | sed 's/[ ]/_/'`
fi
ls -l
cp -r postTreatEVO_tests_results $1/"$MACH"/
echo "-----.save----- postTreatEVO_tests save script terminated"
